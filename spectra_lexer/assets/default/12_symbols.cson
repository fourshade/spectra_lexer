# Rules that actively involve symbols. Still must include at least one alphanumeric character.

{
  "'":   ["AE",    "'",  [], "single quote `'`"],
  "'s":  ["AES",   "'s", [], "possessive suffix `'s`"],
  "s'":  ["AES",   "s'", [], "plural possessive suffix `s'`"],
  ".":   ["-FPLT", ".",  [], "period for abbreviation"]
}
