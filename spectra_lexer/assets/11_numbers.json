  # hundreds
  "~00":  ["-Z",    "00",         "SPEC", "use -Z to produce a multiple of 100"],
  "100":  ["#S-Z",  "(1.)(~00)",  "STRK", "number 100"],
  "200":  ["#T-Z",  "(2.)(~00)",  "STRK", "number 200"],
  "300":  ["#P-Z",  "(3.)(~00)",  "STRK", "number 300"],
  "400":  ["#H-Z",  "(4.)(~00)",  "STRK", "number 400"],
  "500":  ["#AZ",   "(5.)(~00)",  "STRK", "number 500"],
  "600":  ["#-FZ",  "(6.)(~00)",  "STRK", "number 600"],
  "700":  ["#-PZ",  "(7.)(~00)",  "STRK", "number 700"],
  "800":  ["#-LZ",  "(8.)(~00)",  "STRK", "number 800"],
  "900":  ["#-TZ",  "(9.)(~00)",  "STRK", "number 900"],

  # 12 hour time
  "~:00": ["-BG",   ":00",        "SPEC", "use -BG to produce a time (o' clock)"],
  "1:00": ["#S-BG", "(1.)(~:00)", "STRK", "time - 1:00"],
  "2:00": ["#T-BG", "(2.)(~:00)", "STRK", "time - 2:00"],
  "3:00": ["#P-BG", "(3.)(~:00)", "STRK", "time - 3:00"],
  "4:00": ["#H-BG", "(4.)(~:00)", "STRK", "time - 4:00"],
  "5:00": ["#ABG",  "(5.)(~:00)", "STRK", "time - 5:00"],
  "6:00": ["#-FBG", "(6.)(~:00)", "STRK", "time - 6:00"],
  "7:00": ["#-PBG", "(7.)(~:00)", "STRK", "time - 7:00"],
  "8:00": ["#-BLG", "(8.)(~:00)", "STRK", "time - 8:00"],
  "9:00": ["#-BGT", "(9.)(~:00)", "STRK", "time - 9:00"],
  "10:00":["#SOBG", "(10)(~:00)", "STRK", "time - 10:00"],
  "11:00":["#S-BGD","(11)(~:00)", "STRK", "time - 11:00"],
  "12:00":["#ST-BG","(12)(~:00)", "STRK", "time - 12:00"],

  # fractions
  "~/":   ["OEU",   "/",          "SPEC", "use -OEU to produce simple fractions"],
  "1/2":  ["#STOEU","(1)(~/)(2)", "#:NM", "fraction - 1/2"],
  "1/3":  ["#SPOEU","(1)(~/)(3)", "#:NM", "fraction - 1/3"],
  "1/4":  ["#SHOEU","(1)(~/)(4)", "#:NM", "fraction - 1/4"],
