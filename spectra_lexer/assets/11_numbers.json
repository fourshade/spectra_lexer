# Number rules - since each of these is either special or prefixed with the
# number sign, even a large number of them should not hurt lexer performance.

{
  # special rules
  "1":  ["S",     "1",      "SPEC", "1"],
  "2":  ["T",     "2",      "SPEC", "2"],
  "3":  ["P",     "3",      "SPEC", "3"],
  "4":  ["H",     "4",      "SPEC", "4"],
  "5":  ["A",     "5",      "SPEC", "5"],
  "0":  ["O",     "0",      "SPEC", "0"],
  "6":  ["-F",    "6",      "SPEC", "6"],
  "7":  ["-P",    "7",      "SPEC", "7"],
  "8":  ["-L",    "8",      "SPEC", "8"],
  "9":  ["-T",    "9",      "SPEC", "9"],
  
  # single digits
  "1.": ["#S",    "(1)",    "#:NM", "number 1"],
  "2.": ["#T",    "(2)",    "#:NM", "number 2"],
  "3.": ["#P",    "(3)",    "#:NM", "number 3"],
  "4.": ["#H",    "(4)",    "#:NM", "number 4"],
  "5.": ["#A",    "(5)",    "#:NM", "number 5"],
  "0.": ["#O",    "(0)",    "#:NM", "number 0"],
  "6.": ["#-F",   "(6)",    "#:NM", "number 6"],
  "7.": ["#-P",   "(7)",    "#:NM", "number 7"],
  "8.": ["#-L",   "(8)",    "#:NM", "number 8"],
  "9.": ["#-T",   "(9)",    "#:NM", "number 9"],

  # double digits repeated
  "11": ["#S-D",  "[11|1]", "#:NM|d:NM", "number 11"],
  "22": ["#T-D",  "[22|2]", "#:NM|d:NM", "number 22"],
  "33": ["#P-D",  "[33|3]", "#:NM|d:NM", "number 33"],
  "44": ["#H-D",  "[44|4]", "#:NM|d:NM", "number 44"],
  "55": ["#AD",   "[55|5]", "#:NM|d:NM", "number 55"],
  "66": ["#-FD",  "[66|6]", "#:NM|d:NM", "number 66"],
  "77": ["#-PD",  "[77|7]", "#:NM|d:NM", "number 77"],
  "88": ["#-LD",  "[88|8]", "#:NM|d:NM", "number 88"],
  "99": ["#-TD",  "[99|9]", "#:NM|d:NM", "number 99"],

  # double digits distinct
  "12": ["#ST",   "(1)(2)", "#:NM",       "number 12"],
  "13": ["#SP",   "(1)(3)", "#:NM",       "number 13"],
  "14": ["#SH",   "(1)(4)", "#:NM",       "number 14"],
  "15": ["#SA",   "(1)(5)", "#:NM",       "number 15"],
  "10": ["#SO",   "(1)(0)", "#:NM",       "number 10"],
  "16": ["#S-F",  "(1)(6)", "#:NM",       "number 16"],
  "17": ["#S-P",  "(1)(7)", "#:NM",       "number 17"],
  "18": ["#S-L",  "(1)(8)", "#:NM",       "number 18"],
  "19": ["#S-T",  "(1)(9)", "#:NM",       "number 19"],
  "21": ["#STEU", "(2)(1)", "#:NM|EU:NM", "number 21"],
  "23": ["#TP",   "(2)(3)", "#:NM",       "number 23"],
  "24": ["#TH",   "(2)(4)", "#:NM",       "number 24"],
  "25": ["#TA",   "(2)(5)", "#:NM",       "number 25"],
  "20": ["#TO",   "(2)(0)", "#:NM",       "number 20"],
  "26": ["#T-F",  "(2)(6)", "#:NM",       "number 26"],
  "27": ["#T-P",  "(2)(7)", "#:NM",       "number 27"],
  "28": ["#T-L",  "(2)(8)", "#:NM",       "number 28"],
  "29": ["#T-T",  "(2)(9)", "#:NM",       "number 29"],
  "31": ["#SPEU", "(3)(1)", "#:NM|EU:NM", "number 31"],
