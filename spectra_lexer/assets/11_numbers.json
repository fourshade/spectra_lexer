  "82": ["#TEUL", "(8)(2)", "#:NM|EU:NM", "number 82"],
  "83": ["#PEUL", "(8)(3)", "#:NM|EU:NM", "number 83"],
  "84": ["#HEUL", "(8)(4)", "#:NM|EU:NM", "number 84"],
  "85": ["#AEUL", "(8)(5)", "#:NM|EU:NM", "number 85"],
  "80": ["#OEUL", "(8)(0)", "#:NM|EU:NM", "number 80"],
  "86": ["#EUFL", "(8)(6)", "#:NM|EU:NM", "number 86"],
  "87": ["#EUPL", "(8)(7)", "#:NM|EU:NM", "number 87"],
  "89": ["#-LT",  "(8)(9)", "#:NM",       "number 89"],
  "91": ["#SEUT", "(9)(1)", "#:NM|EU:NM", "number 91"],
  "92": ["#TEUT", "(9)(2)", "#:NM|EU:NM", "number 92"],
  "93": ["#PEUT", "(9)(3)", "#:NM|EU:NM", "number 93"],
  "94": ["#HEUT", "(9)(4)", "#:NM|EU:NM", "number 94"],
