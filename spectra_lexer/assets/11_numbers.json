  "46": ["#H-F",  "(4)(6)", "#:NM",       "number 46"],
  "47": ["#H-P",  "(4)(7)", "#:NM",       "number 47"],
  "48": ["#H-L",  "(4)(8)", "#:NM",       "number 48"],
