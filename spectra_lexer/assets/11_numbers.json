  "61": ["#SEUF", "(6)(1)", "#:NM|EU:NM", "number 61"],
  "62": ["#TEUF", "(6)(2)", "#:NM|EU:NM", "number 62"],
  "63": ["#PEUF", "(6)(3)", "#:NM|EU:NM", "number 63"],
