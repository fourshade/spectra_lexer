  "34": ["#PH",   "(3)(4)", "#:NM",       "number 34"],
  "35": ["#PA",   "(3)(5)", "#:NM",       "number 35"],
  "30": ["#PO",   "(3)(0)", "#:NM",       "number 30"],
  "36": ["#P-F",  "(3)(6)", "#:NM",       "number 36"],
  "37": ["#P-P",  "(3)(7)", "#:NM",       "number 37"],
  "38": ["#P-L",  "(3)(8)", "#:NM",       "number 38"],
  "39": ["#P-T",  "(3)(9)", "#:NM",       "number 39"],
  "41": ["#SHEU", "(4)(1)", "#:NM|EU:NM", "number 41"],
