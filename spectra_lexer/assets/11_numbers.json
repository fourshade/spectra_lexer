  "6/7":  ["#OEUFP","(6)(~/)(7)", "#:NM", "fraction - 6/7"],
  "7/8":  ["#OEUPL","(7)(~/)(8)", "#:NM", "fraction - 7/8"],
  "8/9":  ["#OEULT","(8)(~/)(9)", "#:NM", "fraction - 8/9"]
