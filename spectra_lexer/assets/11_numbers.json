  "1/6":  ["#SOEUF","(1)(~/)(6)", "#:NM", "fraction - 1/6"],
  "1/7":  ["#SOEUP","(1)(~/)(7)", "#:NM", "fraction - 1/7"],
  "1/8":  ["#SOEUL","(1)(~/)(8)", "#:NM", "fraction - 1/8"],
  "1/9":  ["#SOEUT","(1)(~/)(9)", "#:NM", "fraction - 1/9"],
  "2/3":  ["#TPOEU","(2)(~/)(3)", "#:NM", "fraction - 2/3"],
  "3/4":  ["#PHOEU","(3)(~/)(4)", "#:NM", "fraction - 3/4"],
  "4/5":  ["#HAOEU","(4)(~/)(5)", "#:NM", "fraction - 4/5"],
